SELECT STNAME FROM STT WHERE (STDEG="ms") AND (STDID=1);
SELECT STMJR FROM STT WHERE (STDID=3) OR (STDEG="phd");
SELECT COTITLE,COCREDIT FROM COT WHERE (CODID=2) AND (COTYPE="P");
