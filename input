CREATE TABLE STT(STID INT,STNAME VARCHAR,STDEG VARCHAR,STMJR VARCHAR,STDID INT);
CREATE TABLE COT(COID INT,COTITLE VARCHAR,CREDIT INT,COTYPE VARCHAR,CODID INT);
CREATE TABLE SCT(STID INT,COID INT,TR IN,YEAR INT,GRADE INT);
CREATE INDEX STID_index ON STT(STID);
CREATE INDEX COID_index ON COT(COID);
